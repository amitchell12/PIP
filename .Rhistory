scale_x_discrete(breaks = c(1,2), labels = c('Early', 'Late')) +
labs(title = 'Bias: time on task', x = '', y = '') +
ylim(-.12,.12) +
theme_classic() +
theme(legend.position = 'none',
axis.title = element_text(size = 14),
axis.text = element_text(size = 12),
title = element_text(size = 14)) -> tonic_bias_plot
# combine plots
EWB_plot <- ggarrange(phasic_bias_plot, tonic_bias_plot,
ncol = 2, nrow = 1)
EWB_plot
ggsave('BiasManipulation-plot.png', EWB_plot, device = NULL, path = aPath,
width = 10, height = 5, dpi = 600)
